{
  "name": "react-boilerplate",
  "version": "0.2.0",
  "description": "Quick packager-agnostic boilerplate for React modules using JSX",
  "main": "lib/index.js",
  "dependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "browserify": "^12.0.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {},
  "scripts": {
    "start": "jsxc --watch src/ lib/",
    "build": "jsxc src/ lib/"
  },
  "files": [
    "lib/",
    "USAGE.txt",
    "static/"
  ],
  "staticRoot": "./static/",
  "staticPlugin": "statics-stylesheets",
  "author": "Pete Hunt",
  "license": "Apache 2"
}
